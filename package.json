{
  "name": "ts-reactor",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/boryszef/ts-reactor.git"
  },
  "scripts": {
    "build": "webpack -p",
    "start": "webpack-dev-server -d --content-base ./public",
    "test": "jest"
  },
  "dependencies": {
    "@types/react-dom": "16.0.11",
    "clean-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "ts-loader": "5.3.3",
    "typescript": "3.2.4",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.13",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "jest": "^24.0.0",
    "react-test-renderer": "^16.7.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  }
}
